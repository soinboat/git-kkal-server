{
  "name": "git-kkal-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "prepare": "husky install",
    "pre-commit": "lint-staged && rm -rf .git/hooks && ln -s ../.husky .git/hooks"
  },
  "dependencies": {
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.12",
    "prettier": "2.3.2"
  },
  "lint-staged": {
    "**/*.{js, jsx, html, css, json}": [
      "prettier --write .",
      "eslint --fix ."
    ]
  }
}
